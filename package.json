{
  "name": "towerism-mc-site",
  "version": "1.0.0",
  "description": "A minecraft server for builders, designers, and KPop lovers",
  "author": "Martin Fracker, Jr.",
  "private": true,
  "scripts": {
    "dev": "nuxt --hostname 0.0.0.0",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --ignore-pattern 'assets/*' ."
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint",
    "*.{css,js}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "copy-to-clipboard": "^3.3.1",
    "node-sass": "^7.0.0",
    "nuxt": "^2.0.0",
    "sass-loader": "^9.0.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "stylelint": "^10.1.0"
  }
}
